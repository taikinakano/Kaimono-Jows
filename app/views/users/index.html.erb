    <div class="container">
    <div class="mt-4">
    <h4 class="mb-3">会員一覧</h4>
     <table class="table">
      <thead class="thead-dark">
      <tr>
      <th>会員画像</th>
      <th>会員名</th>
      <th>紹介文</th>
      <th></th>
      </tr>
     </thead>
     <tbody>
       <% @users.each do |users| %>
       <tr>
         <td>
         <%= link_to user_path(users.id) do %>
         <%= attachment_image_tag users, :image, size: '200x150', formate: 'jpeg', fallback: "no_iamge.jpeg" %>
         <% end %>
         </td>
         <td>
           <%= link_to user_path(users.id) do %>
           <%= users.name %>
           <% end %>
          </td>
          <td><%= users.introduction %></td>
          <td><%= render "users/info", user: users %></td>
       </tr>
       <% end %>
       <%= paginate @users %>
     </tbody>
    </table>
    </div>
    </div>